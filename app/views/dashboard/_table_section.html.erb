<div class='row'>
  <div class='col-xs-10 col-xs-offset-1'>

    <h4>Last 7 Days</h4>


    <table class='table table-striped table-bordered table-condensed'>

      <thead class='bg-primary'>
        <tr>
          <td>Item</td>
          <td>Data</td>
        </tr>
      </thead>

      <tbody>

        <% data.each do |key, value| %>
          <tr>
            <td><%= key %></td>
            <td><%= value %></td>
          </tr>
        <% end %>

      </tbody>

    </table>

  </div> <!-- col -->

</div> <!-- row -->